<!-- gunsight -->
<model>
<name>gunsight</name>
<path>Aircraft/Bell_UH-1H/Models/InteriorUH1/Panel/gunsight.xml</path>
<offsets>
<x-m> -6.975 </x-m>
<y-m>  0.45 </y-m>
<z-m> 0.03 </z-m>
</offsets>
</model>


	<animation>
		<type>select</type>
		<object-name>gunsight</object-name>
		
			<condition>
		 <equals>
    <property>sim/multiplay/generic/string[1]</property>
    <value>guns</value>
   </equals>
   </condition>
	</animation>


