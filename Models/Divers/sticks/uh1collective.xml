<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>uh1collective.ac</path>
	
  

 <animation>
    <name>CollectiveComplet</name>
   <object-name>object.018</object-name>
    <object-name>object.007</object-name>
    <object-name>object.033</object-name>
    <object-name>object.021</object-name>
    <object-name>object.015</object-name>
	 <object-name>llswitch</object-name>
	<object-name>navswitch</object-name>
	<object-name>startswitch</object-name>
  </animation>

 <model>
 <name>llswitch</name>
 <path>Aircraft/Bell_UH-1H/Models/InteriorUH1/Panel/Instruments/switches/landing-lights.xml</path>
 <offsets>
 <x-m>-0.534</x-m>
 <y-m>0.028</y-m>
 <z-m>0.379</z-m>
 <pitch-deg>-59</pitch-deg>
 </offsets>
 </model>
 
 <model>
 <name>navswitch</name>
 <path>Aircraft/Bell_UH-1H/Models/InteriorUH1/Panel/Instruments/switches/nav.xml</path>
 <offsets>
 <x-m>-0.553</x-m>
 <y-m>0.018</y-m>
 <z-m>0.406</z-m>
 <pitch-deg>-46</pitch-deg>
 </offsets>
 </model>
 
  <model>
 <name>startswitch</name>
 <path>Aircraft/Bell_UH-1H/Models/InteriorUH1/Panel/Instruments/Switch/starter.xml</path>
 <offsets>
 <x-m>-0.535</x-m>
 <y-m>0.04</y-m>
 <z-m>0.353</z-m>
 <roll-deg>-90</roll-deg>
 <heading-deg>90</heading-deg>
 </offsets>
 </model>
 
 
 <animation>
    <type>rotate</type>
    <object-name>CollectiveComplet</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <factor> -16 </factor>
    <offsets>
      <x-m>  0.03 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> 0.05</z-m>
    </offsets>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

